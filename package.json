{
  "name": "text-tracker-htmlelement",
  "version": "1.0.0",
  "description": "",
  "main": "text-tracker.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup -c && npm run bookmarklet && npm run replace-bookmarklet",
    "start": "npm run build && serve ./public",
    "bookmarklet": "bookmarklet ./src/text-tracker-loader.js ./public/text-tracker-bookmarklet.js",
    "replace-bookmarklet": "contents=$(<./public/text-tracker-bookmarklet.js) && sed -i '' \"s/href=\"javascript.*\\(\\)\"/$contents/g\" ./public/index.html"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@medv/finder": "^2.1.0",
    "bookmarklet": "^3.0.0",
    "serve": "^14.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.3.0",
    "rollup": "^2.78.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}
